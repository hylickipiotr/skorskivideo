{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "name": "Video"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "title": {
      "type": "string",
      "private": false,
      "required": true,
      "minLength": 5
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 20
    },
    "isPublished": {
      "type": "boolean",
      "default": false,
      "private": false,
      "required": true
    },
    "publishedAt": {
      "type": "datetime",
      "private": false
    },
    "thumbnailUrl": {
      "type": "string",
      "private": false,
      "required": true,
      "minLength": 2
    },
    "tags": {
      "collection": "tag",
      "via": "videos",
      "dominant": true
    }
  }
}
