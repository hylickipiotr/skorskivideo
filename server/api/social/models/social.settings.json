{
  "kind": "collectionType",
  "collectionName": "socials",
  "info": {
    "name": "Social"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "string",
      "required": true
    },
    "isPublished": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "publishedAt": {
      "type": "datetime"
    },
    "label": {
      "type": "string",
      "required": true
    },
    "colorHex": {
      "type": "string",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    }
  }
}
