{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "name": "Faq"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "question": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "answer": {
      "type": "richtext",
      "required": true,
      "minLength": 5
    },
    "isPublished": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "publishedAt": {
      "type": "datetime"
    }
  }
}
